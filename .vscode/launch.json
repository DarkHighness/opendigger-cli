    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'opendigger-cli'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=opendigger-cli",
                    "--package=opendigger-cli"
                ],
                "filter": {
                    "name": "opendigger-cli",
                    "kind": "bin"
                }
            },
            "args": [
                "sql",
                "select * from Openrank a, Openrank b where a.name = 'X-lab2017/open-digger' and b.name ='X-lab2017/open-digger'"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'opendigger-cli'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=opendigger-cli",
                    "--package=opendigger-cli"
                ],
                "filter": {
                    "name": "opendigger-cli",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}